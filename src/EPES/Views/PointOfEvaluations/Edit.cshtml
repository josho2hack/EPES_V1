    <script type="text/javascript">

    var theme_arr = JSON.parse('@Html.Raw(Json.Serialize(@ViewBag.Theme))');
    var end_arr = JSON.parse('@Html.Raw(Json.Serialize(ViewBag.End))');
    var way_arr = JSON.parse('@Html.Raw(Json.Serialize(ViewBag.Way))');

    $(document).ready(function() {
        var ii;
        var themeendway_option = "<option value=\"\">--- โปรดเลือก ---</option>";
        var theme_option = themeendway_option;
        for (ii = 0; ii < theme_arr.length; ii++)
        {
            theme_option += '<option value="' + theme_arr[ii]["id"] + '">' + theme_arr[ii]["themeName"] + "</option>";
        }
        $("#point_ThemeId").on('change', function () {
            var end_option = themeendway_option;
            for (ii = 0; ii < end_arr[$(this).val()].length; ii++) {
                end_option += '<option value="' + end_arr[$(this).val()][ii][0] + '">' + end_arr[$(this).val()][ii][1] + "</option>";
            }
            $("#point_EndId").html(end_option);
            $("#point_WayId").html(themeendway_option);
        });

        $("#point_EndId").on('change', function () {
            var way_option = themeendway_option;
            for (ii = 0; ii < way_arr[$(this).val()].length; ii++) {
                way_option += '<option value="' + way_arr[$(this).val()][ii][0] + '">' + way_arr[$(this).val()][ii][1] + "</option>";
            }
            $("#point_WayId").html(way_option);
        });

        @if(Model.point.ThemeId == null)
        {
            <text>
                $("#point_ThemeId").html(theme_option);
                $("#point_EndId").html(theme_option);
                $("#point_WayId").html(themeendway_option);
            </text>
        } else
        {
            <text>
                $("#point_ThemeId").html(theme_option).val(@Model.point.ThemeId).trigger('change');
            </text>
                @if(Model.point.EndId != null)
                {
                    <text>
                        $("#point_EndId").val(@Model.point.EndId).trigger('change');
                    </text>
                }

                @if(Model.point.WayId != null) {
                    <text>
                        $("#point_WayId").val(@Model.point.WayId);
                    </text>
                }
        }

        @if (Model.point.WeightAll)
        {
            <text>
                $("#weight10").prop("disabled", true);
                $("#weight11").prop("disabled", true);
                $("#weight12").prop("disabled", true);
                $("#weight1").prop("disabled", true);
                $("#weight2").prop("disabled", true);
                $("#weight3").prop("disabled", true);
                $("#weight4").prop("disabled", true);
                $("#weight5").prop("disabled", true);
                $("#weight6").prop("disabled", true);
                $("#weight7").prop("disabled", true);
                $("#weight8").prop("disabled", true);
                $("#weight9").prop("disabled", true);
            </text>
        }

    });
